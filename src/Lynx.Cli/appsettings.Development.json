        "logger": "*",
        "minLevel": "Off",
        //"minLevel": "Trace",
        "writeTo": "moves"
      },

      "100": {
        "logger": "*",
        //"minLevel": "Warn",
        "minLevel": "Debug",
        "writeTo": "console"
      }
    }


