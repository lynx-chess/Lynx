{
  "GeneralSettings": {
    "EnableTuning": true
  },
  "NLog": {
    "internalLogLevel": "Warn",
    "throwExceptions": true,
    "rules": {

      "1": {
        "logger": "*",
        //"minLevel": "Off",
        "minLevel": "Debug",
        "writeTo": "logs"
      },

      "2": {
        "logger": "*",
        "minLevel": "Off",
        //"minLevel": "Trace",
        "writeTo": "moves"
      },

      "100": {
        "logger": "*",
        //"minLevel": "Warn",
        "minLevel": "Debug",
        "writeTo": "console"
      }
    }
  }
}
